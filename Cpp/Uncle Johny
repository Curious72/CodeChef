#include<iostream>
#include<cstdio>
#include<algorithm>
using namespace std;
int main()
{
    int t;
    scanf("%d",&t);
    while(t>0)
    {
        int h,i;
        scanf("%d",&h);
        int A[h];
        for(i=0;i<h;i++)
        scanf("%d",&A[i]);
        int uj;
        scanf("%d",&uj);
        int b=A[uj-1];
        sort(A,A+h);
        for(i=0;i<h;i++)
        {
            if(A[i]==b)
            {
                printf("%d \n",i+1);
                break;
            }
        }
 
 
 
 
 
 
 
 
        t--;
    }
 
 
 
 
