#include <iostream>
#include<cstdio>
using namespace std;
 
int main()
{
    int h,y;
    scanf("%d %d",&h,&y);
    int dif,mod;
    dif=h-y;
    mod=dif%10;
    if((mod-1)>0)
    {
    printf("%d",(dif-1));
    }
        else
        {
            printf("%d",(dif+1));
        }
 
 
        h--;
    }
 
