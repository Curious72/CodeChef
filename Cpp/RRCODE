    #include<cstdio>
    #include<iostream>
    #include<string.h>
    #include<cstring>
    using namespace std;
    int main()
    {
        int t;
        scanf("%d",&t);
        while(t--)
        {
            unsigned long long int n,k,answer;
     
            scanf("%llu %llu %llu",&n,&k,&answer);
            int i=0;
            int A[n];
            for(i=0;i<n;i++)
            {
                scanf("%d",&A[i]);
     
            }
            string p;
            cin>>p;
            if(k==0)
            {
                cout<<answer<<endl;
            }
            else
            {
     
            if(p.compare("XOR")==0)
            {
             if(k&1)
             {
                 for(i=0;i<n;i++)
                    answer=answer^A[i];
             }
             else
                answer=0^answer;
             cout<<answer<<endl;
            }
            else if(p.compare("OR")==0)
            {
                for(i=0;i<n;i++)
                    answer=answer|(A[i]);
     
            cout<<answer<<endl;
     
            }
            else
            {
     
                for(i=0;i<n;i++)
                    answer=answer&A[i];
     
            cout<<answer<<endl;
     
     
            }
     
     
     
        }
     
     
        }
     
     
     
    }
     
