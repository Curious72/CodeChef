#include<cstdio>
#include<set>
#include<iostream>
using namespace std;
int main()
{
 
int t;
scanf("%d",&t);
while(t--)
{
    int n;
    scanf("%d",&n);
    set<int> h;
    int i;
    for(i=0;i<n;i++)
    {
    int p;
    scanf("%d",&p);
    h.insert(p);
    }
printf("%d \n",h.size());
 
 
 
}
 
 
 
 
 
}
 
