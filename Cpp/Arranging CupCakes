#include<iostream>
#include<cstdio>
#include<math.h>
#include<cmath>
using namespace std;
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n;
        scanf("%d",&n);
        int u=sqrt(n);
 
        int i,num1,num2;
        int p=0;
        for(i=2;i<=u;i++)
        {
            if(n%i==0)
            {
                num1=i;
                num2=n/i;
                p=1;
            }
 
 
        }
        if(p==1)
        {
            printf("%d \n",(num2-num1));
 
 
        }
        else
            printf("%d \n",(n-1));
 
 
    }
 
 
 
 
}
 
