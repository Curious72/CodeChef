#include<iostream>
#include<cstdio>
using namespace std;
int main()
{
    int h;
    scanf("%d",&h);
    string s="zyxwvutsrqponmlkjihgfedcba";
    while(h>0)
    {
        int r,i;
        scanf("%d",&r);
        int y=r/25;
int e=r%25;
        if(e>0)
        {
 
            for(i=25-e;i<=25;i++)
                cout<<s[i];
 
 
 
        }
 
 
            for(i=0;i<y;i++)
                cout<<s;
cout<<endl;
      h--;
        }
        return 0;
}
