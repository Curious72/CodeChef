#include<iostream>
#include<cstdio>
using namespace std;
int main()
{
 
    int t;
    scanf("%d",&t);
    while(t>0)
    {
        int y,i,count=1;
        scanf("%d",&y);
        long long unsigned int prev;
        scanf("%llu",&prev);
        for(i=1;i<y;i++)
        {
            long long unsigned int h;
            scanf("%llu",&h);
            if(h<prev)
            {
                prev=h;
                count=count+1;
 
 
 
            }
 
 
 
 
        }
        printf("%d \n",count);
 
 
 
 
 
 
 
 
 
 
 
        t--;
    }
 
 
 
}
 
