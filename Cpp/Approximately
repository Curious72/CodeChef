#include<cstdio>
#include<iostream>
#include<iomanip>
#include<string>
#include<string.h>
#include<cstring>
using namespace std;
int main()
{
    int t;
    scanf("%d",&t);
 
    while(t--)
    {
        int k;
 
        scanf("%d",&k);
        if(k==0)
            printf("3 \n");
        else
        {
             printf("3.");
            int i=0;
            int a=103993%33102 ;
            int b=33102;
            int g,h;
 
            for(i=0;i<k;i++)
            {
                 g=(a*10)/b;
                 h=(a*10)%b;
                 a=h;
                printf("%d",g);
 
            }
 
printf("\n");
 
 
        }
 
 
 
 
 
    }
 
 
 
 
 
 
}
 
