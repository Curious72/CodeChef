#include<iostream>
#include<cstdio>
using namespace std;
int main()
{
 
    int t;
    scanf("%d",&t);
    while(t>0)
    {
        int a,b;
        scanf("%d %d",&a,&b);
        if(a>b)
            printf("%d ",a);
        else
            printf("%d ",b);
        printf("%d \n",(a+b));
 
 
 
 
 
        t--;
    }
 
