#include<iostream>
#include<cstdio>
using namespace std;
int main()
{
    long long int n;
    scanf("%lld",&n);
    long long int i=0;
    long long int sum=0;
 
    for(i=0;i<n;i++)
    {
            long long int a;
            scanf("%lld",&a);
            sum=sum+a;
 
    }
    long long unsigned int y=(n*(n+1))/2;
    if(sum==y)
        printf("YES \n");
    else
        printf("NO \n");
 
 
 
 
 
 
}
