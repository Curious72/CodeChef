#include <iostream>
#include <cstdio>
using namespace std;
 
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        string s;
        cin>>s;
        int length;
        length=s.length();
        int i,g=0;
        for(i=0;i<length-2;i++)
        {
            if(s[i+1]!=s[i])
            {
 
                if(s[i+2]==s[i])
                {
                   g=1;
                   break;
                }
 
 
            }
 
 
 
        }
        if(g==1)
            printf("Good \n");
        else
            printf("Bad \n");
 
 
 
 
 
    }
}
