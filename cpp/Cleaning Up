    #include<iostream>
    #include<cstdio>
    #include<algorithm>
    using namespace std;
    int main()
    {
     
        int t,i;
        scanf("%d",&t);
        while(t>0)
        {
            int n,m;
            scanf("%d %d",&n,&m);
        int A[m],B[n],C[n];
    for(i=0;i<m;i++)
        scanf("%d",&A[i]);
    sort(A,A+m);
    int g=1,c=0,length1=0,length2=0;
    for(i=1;i<=n;i++)
    {
        if(g%2)
        {
          if(i!=A[c])
          {
              B[length1]=i;
              length1++;
          }
          else
          {
              c++;
              g=g-1;
          }
        }
        else
        {
            if(i!=A[c])
          {
              C[length2]=i;
              length2++;
          }
          else
          {
              c++;
              g=g-1;
          }
     
     
     
        }
        g++;
    }
     
    for(i=0;i<length1;i++)
        printf("%d ",B[i]);
    printf("\n");
    for(i=0;i<length2;i++)
    printf("%d ",C[i]);
    printf("\n");
     
     
            t--;
        }
     
     
     
     
     
     
     
    }
     
